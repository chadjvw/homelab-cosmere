{
  $schema: "https://docs.renovatebot.com/renovate-schema.json",
  extends: [
    "docker:enableMajor",
    ":disableRateLimiting",
    ":dependencyDashboard",
    ":semanticCommits",
    ":semanticCommitTypeAll(chore)",
    ":skipStatusChecks",
    // "schedule:weekly",
    "github>chadjvw/homelab-cosmere//.github/renovate/package-rules.json5",
    "github>chadjvw/homelab-cosmere//.github/renovate/commit-message.json5",
    "github>chadjvw/homelab-cosmere//.github/renovate/semantic-commits.json5",
    "github>chadjvw/homelab-cosmere//.github/renovate/custom-managers.json5",
  ],
  enabled: true,
  rebaseWhen: "conflicted",
  onboarding: false,
  requireConfig: "ignored",
  "pre-commit": {
    enabled: false,
  },
  ignorePaths: ["deploy/**", "components/**/vendor/**"],
  kubernetes: {
    managerFilePatterns: ["/\\.yaml$/"],
  },
}
